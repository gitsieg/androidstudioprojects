<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="14dp"
    android:layout_margin="14dp"
    android:orientation="vertical"
    android:id="@+id/main_layout"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="xyz.gitsieg.recyclerview2.Fragmentversjon.Main2Activity">

    <Button
        android:id="@+id/fylkeKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/vis_fylke"/>

    <Button
        android:id="@+id/bildeKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/visBilder"/>

    <Button
        android:id="@+id/kommuneKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/vis_kommuner"/>

</LinearLayout>
