<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="14dp"
    android:layout_margin="14dp"
    android:orientation="vertical"
    android:id="@+id/main_layout"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="xyz.gitsieg.recyclerview2.Aktivitetsversjon.MainActivity"
    tools:showIn="@layout/activity_main">
    

    
    <Button
        android:id="@+id/btn_velgFylke"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onFylkerClick"
        android:text="@string/vis_fylke"/>

    <Button
        android:id="@+id/btn_visBilder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onBilderClicked"
        android:text="@string/visBilder"/>

    <Button
        android:id="@+id/btn_visKommuner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onKommunerClicked"
        android:text="@string/vis_kommuner"/>



</LinearLayout>
