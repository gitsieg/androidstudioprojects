<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:id="@+id/fragment_container_land"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="xyz.gitsieg.recyclerview2.Fragmentversjon.Main2Activity"
    tools:showIn="@layout/activity_main2">

    <fragment
        android:name="xyz.gitsieg.recyclerview2.Fragmentversjon.KnappeFragment"
        android:id="@+id/knappe_fragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1" />

    <fragment
        android:name="xyz.gitsieg.recyclerview2.Fragmentversjon.FylkeListeFragment"
        android:id="@+id/fylke_fragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"/>
</LinearLayout>
